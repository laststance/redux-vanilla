{
  "name": "redux-vanilla",
  "version": "0.0.1-beta2",
  "description": "Zero Abstraction React Redux binding library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "index.d.ts",
  "files": [
    "es",
    "lib",
    "src",
    "index.d.ts"
  ],
  "keyword": [
    "react",
    "reactjs",
    "flux",
    "redux"
  ],
  "scripts": {
    "flow": "flow",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:watch:verbose": "jest --coverage --watch --verbose",
    "codecov": "codecov -t $CODECOV_TOKEN",
    "build:commonjs": "NODE_ENV=cjs rollup -c -o lib/index.js",
    "build:es": "NODE_ENV=es rollup -c -o es/index.js",
    "build": "yarn build:commonjs && yarn run build:es",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryota-murakami/redux-vanilla.git"
  },
  "author": "ryota-murakami <dojce1048@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryota-murakami/redux-vanilla/issues"
  },
  "homepage": "https://github.com/ryota-murakami/redux-vanilla#readme",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/example/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/testutils.js"
    ]
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.70.0",
    "jest": "^22.4.2",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "redux": "^3.7.2",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {}
}
