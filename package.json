{
  "name": "redux-vanilla",
  "version": "0.0.1",
  "description": "react redux vailla binding.",
  "main": "build/index.js",
  "jsnext:main": "build/index.esm.js",
  "module": "build/index.esm.js",
  "types": "index.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "yarn build && yarn build:flow",
    "build": "babel src -d build --ignore '**/*.test.js'",
    "rollup": "rollup -c",
    "build:flow": "flow-copy-source -v -i '*.test.js' src build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryota-murakami/redux-vanilla.git"
  },
  "author": "ryota-murakami <dojce1048@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryota-murakami/redux-vanilla/issues"
  },
  "homepage": "https://github.com/ryota-murakami/redux-vanilla#readme",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "jest": "^22.4.2",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.7.2",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.2.0"
  },
  "dependencies": {
    "create-react-context": "^0.2.1"
  }
}
