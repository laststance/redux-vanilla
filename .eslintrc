{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "plugins": [
    "prettier",
    "flowtype"
  ],
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true,"semi": false, "parser": "flow"}],
    "no-unused-vars": ["error"],
    "react/prop-types": false,
    "flowtype/define-flow-type": 1
  }
}